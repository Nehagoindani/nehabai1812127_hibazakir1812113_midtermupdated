<!DOCTYPE html>
<html>
  <head>
   
  </head>
  <body>
   
     <div class="container-fluid bg-3 text-center">    
     
      <div class="row">
        <table class='table table-striped table-responsive-md btm-table'>
          <thead>
              <tr>
                  <th>name</th>
                  <th>Instititute_name</th>
                  <th>Program</th>

                  <th>&nbsp;</th>
                  <th>&nbsp;</th>
              </tr>
          </thead>
          <tbody>
              <% Cv.forEach(function(Cv){ %>
                  <tr>
                      <td><%=Cv._id %></td>
                      <td><%=Cv.name %></td>
                      <td><%=Cv.Instititute_name%></td>
                      <td><%=Cv.Program%></td>
                      <td><%=Cv.start_Date1%></td>
                      <td><%=Cv.End_Date1%></td>
                      <td><%=Cv.Field%></td>
                      <td><%=Cv.Languages%></td>
                      <td><%=Cv.activity%></td>
                      <td><%=Cv.Project_name %></td>

                      <td>
                          <form action='main/<%=Cv._id %>' method="POST">
                              <button type="submit">Update</button>
                          </form>
                      </td>
                      <td>
                      <form action='<%=Cv._id %>/delete/' method="POST">
                          <button type="submit">Delete</button>
                      </form>
                  </td>                   
                  </tr>
               <% });  %>   
          </tbody>
      </table>


      </div>
    </div>
    </body>
   
    
  
</html> 